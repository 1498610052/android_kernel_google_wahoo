&pm8998_gpios {
        /* dac,power-gpio */
	gpio@c900 {
		qcom,master-en = <1>;   	/* ENABLE GPIO */
		qcom,mode = <1>;		/* Digital output */
		qcom,pull = <4>;		/* Pull down */
		qcom,src-sel = <0>;		/* Constant */
		qcom,invert = <0>;		/* Invert */
		qcom,out-strength = <1>;	/* Low */
		status = "okay";
	};

        /* dac,switch-gpio */
	gpio@cb00 {
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,mode = <1>;		/* Digital output */
		qcom,pull = <4>;		/* Pull down */
		qcom,src-sel = <0>;		/* Constant */
		qcom,invert = <0>;		/* Invert */
		qcom,out-strength = <1>;	/* Low */
		status = "okay";
	};

	/* tas2557, cdc-reset-gpio */
	gpio@c500 { /* GPIO 6 */
		qcom,mode = <1>;       		/* Output */
		qcom,pull = <5>;		/* No Pull */
		qcom,vin-sel = <0>;		/* VIN1 GPIO_LV */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,out-strength = <1>;	/* Low */
		status = "ok";
	};

	/* tas2557, irq-gpio-right */
	gpio@c600 { /* GPIO 7 */
		qcom,mode = <1>;       		/* Output */
		qcom,pull = <5>;		/* No Pull */
		qcom,vin-sel = <0>;		/* VIN1 GPIO_LV */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,out-strength = <1>;	/* Low */
		status = "ok";
	};

	/* tas2557, irq-gpio-left */
	gpio@c700 { /* GPIO 8 */
		qcom,mode = <1>;                /* Output */
		qcom,pull = <5>;                /* No Pull */
		qcom,vin-sel = <0>;		/* VIN1 GPIO_LV */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,out-strength = <1>;	/* Low */
		status = "ok";
	};

};

&rpm_bus {
	rpm-regulator-ldoa19 {
		status = "okay";
		regulator-l19 {
			regulator-min-microvolt = <3008000>;
			regulator-max-microvolt = <3008000>;
			qcom,init-voltage = <3008000>;
			qcom,vdd-voltage-level = <3008000 3008000>;
			regulator-always-on;
			status = "okay";
		};
	};
